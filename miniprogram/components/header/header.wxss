.header{
  width: 100%;
  background-color: rgb(255, 255, 255);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}
.search{
  font-size: 14px;
  text-indent: 15px;
  color: #929292;
  position: absolute;
  width: 65vw;
  left: 20px;
  border-radius: 100px;
  background-color: #e4e4e4;
}
.leftBtn{
  box-sizing: border-box;
  border-radius: 100px;
  border:0.5px solid #e9e9e9;
  position: absolute;
  left: 20px;
}
.back{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 10px;
}
.mid{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  width: 1px;
  background-color: #e9e9e9;
}
.home{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 10px;
}